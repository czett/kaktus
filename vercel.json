{
    "version": 2,
    "builds": [
        {
            "src": "Kakteen/flask_backend/app.py",
            "use": "@vercel/python",
            "excludeFiles": "**/*.html"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Kakteen/flask_backend/app.py"
        }
    ]
}
